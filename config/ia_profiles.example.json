    "_usage_examples": {
        "_comment": "Ejemplos de uso del IAConfigManager",
        "example_1_basic_usage": {
            "description": "Uso básico - obtener perfil para bot",
            "code": "manager = IAConfigManager(config); profile = manager.get_profile_for_bot('bot_1_numerico')"
        },
        "example_2_switch_profile": {
            "description": "Cambiar perfil de un bot dinámicamente",
            "code": "manager.switch_profile('bot_1_numerico', 'gpt-4-turbo')"
        },
        "example_3_validate_quota": {
            "description": "Validar cuota antes de usar perfil",
            "code": "profile = manager.get_profile_for_bot('bot_1', validate_quota=True, auto_fallback=True)"
        },
        "example_4_track_costs": {
            "description": "Registrar uso y calcular costos",
            "code": "manager.track_usage('bot_1', 'gemini-pro', tokens_used=1500); stats = manager.get_usage_stats('bot_1')"
        },
        "example_5_compare_bots": {
            "description": "Comparar costos entre bots",
            "code": "comparison = manager.get_cost_comparison(['bot_1', 'bot_2', 'bot_3'])"
        }
